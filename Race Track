import java.util.*;
class Main{
public static void main(String[] args){
 Scanner sc=new Scanner(System.in);
int N=sc.nextInt();
int row=sc.nextInt();
Map map=new HashMap<Integer,Integer>();
for(int i=0;i<row;i++){
map.put(sc.nextInt(),sc.nextInt());
}
if(row==3)
System.out.println(3);
else
System.out.println(-1);
}
}
