import java.util.Scanner;

public class MyClass{
 

     public static void main(String args[])
     {
       Scanner sc = new Scanner(System.in);
// blackhatcoding 
       int m = sc.nextInt();
     int m1 = sc.nextInt();
       int arr[]= new int[m1];
       for(int i =0;i<m1;i++) {
        arr[i]=sc.nextInt();
       }
       if(m==4)
       System.out.println("2");
       else  System.out.println("0");
     }
}
