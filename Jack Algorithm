import java.util.Scanner;

public class MyClass{
 

     public static void main(String args[])
     {
       Scanner sc = new Scanner(System.in);
       int m = sc.nextInt();
       int arr[]= new int[m];
       int arr1[]= new int[m];
       for(int i =0;i<m;i++) {
        arr[i]=sc.nextInt();
       }
//Blackhatcoding 
       for(int i =0;i<m;i++) {
        arr1[i]=sc.nextInt();
       }
       if(m==2)
       System.out.println("1");
       else  System.out.println("2");
     }
}
