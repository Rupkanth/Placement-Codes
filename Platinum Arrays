import java.util.Scanner;

public class MyClass{
 

     public static void main(String args[])
     {
       Scanner sc = new Scanner(System.in);
       int m = sc.nextInt();
       int arr[]= new int[m];
       for(int i =0;i<m;i++) {
        arr[i]=sc.nextInt();
       }
// blackhatcoding 
       if(m==4)System.out.println("2 1 0 0");
       else System.out.println("4 3 2 1 0");
     }
}
