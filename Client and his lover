def prime(n):
   n=int(n) 
   if(n==1):
       return 0 
   elif(n==2):
       return 1
   else: 
      for i in range(2,n//2+1): 
         if(n%i==0):
           return 0 
      return 1 
def lcm(n):
  p=0
  i=2 
  while (n!=1):
    if(n%i==0):
     n=n//i 
    else:
     i+=1 
  return p 
n=int(input()
while(n!=0):
  if(prime(n)):
     n-=1
     c+=1 
  else:
    n=lcm(n)
    c+=1 
    print(c)

