def abc(n):
 count = [0] * (n+1)
 count[0] = 1
 count[1] = 1
 for i in range (2, n+1):
   count[i] = 0; 
   if (input1[i-1]> '0'):
     count[i] = count[i-1] 
   if (input1[i - 2] = '1' or (input1[1-2] == '2' and input1[i-1] < 7')):
     count [i] += count[i -2]
   return count[n] 
input1 = input()
 n = len (input1) 
abc (n) 

