double calculateIncome(double income)
{
    if (income < 250000)
        return income;
    else if (income < 500000)
        return income * 0.05;
    else if (income < 750000)
        return income * 0.1;
    else if (income < 1000000)
        return income * 0.15;
    else if (income < 1250000)
        return income * 0.20;
    else if (income < 1500000)
        return income * 0.25;
    else
        return income * 0.30;
}
