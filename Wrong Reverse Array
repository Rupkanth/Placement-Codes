int reversearray(int array[], int n) {
        int j = 0, tmp = 0;
        for (int i = n-1; i >= n/2; --i ) {
                tmp = array[j];
                array[j] = array[i];
                array[i] = tmp;
                ++j;
        }

        /*for (int i = 0; i < n; ++i)
                cout << array[i] << " ";*/

        return 0;
}
